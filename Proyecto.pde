      if(this.character == 1){
        switch (this.poder){
          case 1:
            this.texto = "Predator Shot";
            break;
                   
          case 2:
            this.texto = "Omni-Blade Slash";
            break;
            
          case 3:
            this.texto = "Biotic Warp";
            break;
        }
      }
      else if(this.character == 2){
